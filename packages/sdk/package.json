{
    "name": "@sovseal/sdk",
    "version": "1.0.0",
    "description": "TypeScript SDK for the SovSeal Protocol - decentralized digital legacy and time-locked media",
    "license": "Apache-2.0",
    "author": "SovSeal Protocol Contributors",
    "repository": {
        "type": "git",
        "url": "https://github.com/sovseal/sdk"
    },
    "homepage": "https://sovseal.io",
    "keywords": [
        "sovseal",
        "digital-legacy",
        "time-lock",
        "encryption",
        "blockchain",
        "web3",
        "typescript"
    ],
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        },
        "./crypto": {
            "types": "./dist/crypto/index.d.ts",
            "import": "./dist/crypto/index.mjs",
            "require": "./dist/crypto/index.js"
        }
    },
    "files": [
        "dist",
        "src",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts",
        "test": "vitest",
        "lint": "eslint src --ext .ts",
        "typecheck": "tsc --noEmit"
    },
    "peerDependencies": {
        "ethers": "^6.0.0"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "tsup": "^8.0.0",
        "typescript": "^5.0.0",
        "vitest": "^1.0.0"
    }
}